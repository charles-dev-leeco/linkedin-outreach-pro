{
  "name": "linkedin-outreach-pro",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "clean": "turbo run clean",
    "web": "cd apps/web && npm run dev",
    "api": "cd apps/api && npm run dev",
    "extension": "cd apps/extension && npm run build",
    "start": "node apps/api/dist/server.js"
  },
  "devDependencies": {
    "turbo": "^1.11.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
