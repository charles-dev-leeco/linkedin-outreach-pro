{
  "buildCommand": "cd ../.. && npm install && npm run build --workspace=apps/web",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    { "source": "/api/(.*)", "destination": "https://linkedin-outreach-pro-api.up.railway.app/api/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}